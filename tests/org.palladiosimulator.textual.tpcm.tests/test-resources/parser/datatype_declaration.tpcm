repository PrimitiveDataTypeRepository {
	datatype DT1 INT
}

repository ComposedDataTypeRepository {
	datatype CDT1 {
		element1 PrimitiveDataTypeRepository::DT1
		element2 datatype CollectionDT1 []<PrimitiveDataTypeRepository::DT1>
	}
	datatype CDT2 {
		element1 CDT1::CollectionDT1
		element2 CDT1
		element3 datatype PDT1 DOUBLE 
	}
}